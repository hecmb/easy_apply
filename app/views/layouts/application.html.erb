<!DOCTYPE html>
<html>
  <head>
    <title>EasyApply</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js" %>



    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <%= javascript_include_tag "//www.google.com/jsapi" %>
  </head>

  <body>
  	<%= link_to "Home Page", root_path, class: "btn btn-default", :style => 'margin:20px 0px 0px 20px;color:black;' if !current_page?(root_path)%>

  	<p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
